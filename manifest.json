{
	"manifest_version": 2,
	"name":"quick location",
	"description":"quick location when you are searching",
	"version":"1.0",
	"permissions":[],
	"browser_action":{
		"default_icon":"icon.png",
		"default_popup":"popup.html"
	},
	"content_scripts":[
		{
			"matches":["http://*/*", "https://*/*"],
			"js":["jquery-1.10.2.min.js", "contentscript.js"],
			"css":["content.css"]
		}
	]	
}
